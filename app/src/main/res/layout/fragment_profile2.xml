<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/teti"
    android:layout_height="match_parent"
    tools:context=".profile2">


    <TextView
        android:layout_width="339dp"
        android:layout_height="64dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="64dp"
        android:text=" BANGUN RUANG"
        android:textColor="@color/black"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/persegdatar"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="156dp"
        android:src="@drawable/block"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/segitigadatr"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="56dp"
        android:src="@drawable/triangle3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/persegdatar" />

    <ImageView
        android:id="@+id/persegipanjangdatr"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="312dp"
android:src="@drawable/square2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/jardat"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/wada"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/segitigadatr" />

    <ImageView
        android:id="@+id/bulatRUNG"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="224dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/sphere2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/persegipanjangdatr" />


</androidx.constraintlayout.widget.ConstraintLayout>
